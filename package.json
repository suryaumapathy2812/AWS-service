    "build": "npm run build:esm && npm run build:cjs",
    "prepublish": "npm run build"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/suryaumapathy2812/AWS-service.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^16.4.2",
    "aws-sdk": "^2.953.0",
    "dotenv": "^10.0.0"
  }
}