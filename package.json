{
  "name": "@suryaumapathy2812/aws-service",
  "version": "1.2.0",
  "description": "Node JS Client for AWS-SDK",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/aws-service.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "surya_umapathy",
  "size-limit": [
    {
      "path": "dist/aws-service.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/aws-service.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.2",
    "husky": "^7.0.1",
    "size-limit": "^5.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "aws-sdk": "^2.954.0"
  }
}