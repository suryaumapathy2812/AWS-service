{
  "name": "@suryaumapathy2812/aws-service",
  "version": "1.0.1",
  "description": "Node JS Client for AWS-SDK",
  "type": "module",
  "main": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "module": "./dist/esm/index.js",
  "files": [
    "./dist"
  ],
  "scripts": {
    "watch": "tsc --watch",
    "build:esm": "tsc -p tsconfig.json",
